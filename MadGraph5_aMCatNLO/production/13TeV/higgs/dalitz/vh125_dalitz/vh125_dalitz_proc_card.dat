set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model sm
define p = g u c d s u~ c~ d~ s~
define j = g u c d s u~ c~ d~ s~
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
import model ANO_HEFT_JGF_v2
generate p p > z > h z QCD=0 ANO=0, h > mu+ mu- a
add process p p	> w- > h w- QCD=0 ANO=0, h > mu+ mu- a
add process p p	> w+ > h w+ QCD=0 ANO=0, h > mu+ mu- a
output vh125_dalitz
